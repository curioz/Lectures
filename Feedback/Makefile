
default:	Review_UoA_PGT_UG.pdf Review_Journals.pdf Feedback_2ndY_XiaoLiang.pdf

Review_UoA_PGT_UG.pdf:	Review_UoA_PGT_UG.tex
Review_Journals.pdf:	Review_Journals.tex
Feedback_2ndY_XiaoLiang.pdf:	Feedback_2ndY_XiaoLiang.tex

%.pdf:	%.tex
	rm -f $*.pdf
	pdflatex $<
	pdflatex $<
	if fgrep "Rerun to" $*.log; then $(MAKE) --assume-new $< $@;fi
	@echo Sticking a cover on $*.pdf

.PHONY: clean clean-pdf

# We don't rm *.ps because that might clobber image files. Instead, put any
# ps files which should go in $(PSFILES)
clean:	
	rm $(TARGETS)*.aux *.log *pdf *out
clean-pdf: 
		rm *.pdf
